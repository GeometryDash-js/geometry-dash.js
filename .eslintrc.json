{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "prefer-const": "warn",
        "max-params": "warn",
        "no-array-constructor": "warn",
        "no-const-assign": "warn",
        "no-continue": "warn",
        "no-multi-assign": "warn",
        "no-unused-expressions": "warn",
        "no-undefined": "warn"
    }
}